{
  "name": "ngo-site",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "test": "jest --runInBand"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "whatwg-fetch": "3.6.20"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": ["**/__tests__/**/*.test.js"],
    "roots": ["."],
    "moduleFileExtensions": ["js", "json"],
    "setupFiles": ["<rootDir>/test/setupJest.js"]
  }
}
